# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Project Configuration
PROJECT_NAME=ai-career-agent
ENVIRONMENT=prod

# Bedrock Configuration
BEDROCK_AGENT_ID=your_agent_id_here
BEDROCK_AGENT_ALIAS_ID=your_alias_id_here

# S3 Bucket Names (will be created by CloudFormation)
DOCUMENT_BUCKET=ai-career-agent-documents-prod
JOB_DATA_BUCKET=ai-career-agent-job-data-prod

# External API Keys (optional)
INDEED_API_KEY=your_indeed_api_key
LINKEDIN_API_KEY=your_linkedin_api_key
GLASSDOOR_API_KEY=your_glassdoor_api_key

# Email Configuration
NOTIFICATION_EMAIL=your-email@example.com